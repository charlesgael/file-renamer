{
    "name": "file-renamer",
    "version": "0.0.1",
    "license": "GPL-3.0-or-later",
    "description": "The best TV and Movie Renamer",
    "scripts": {
        "dev": "electron-webpack dev",
        "compile": "electron-webpack",
        "dist": "yarn compile && electron-builder",
        "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
        "icons": "node ./scripts/svg2ico.js",
        "mksvg": "svgr -d src/components/SvgComponents/ svg/"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.8",
        "@material-ui/lab": "^4.0.0-alpha.47",
        "@types/react": "^16.9.31",
        "@types/react-dom": "^16.9.6",
        "electron-util": "^0.14.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-icons": "^3.9.0",
        "source-map-support": "^0.5.12"
    },
    "devDependencies": {
        "@fiahfy/icns-convert": "^0.0.8",
        "@svgr/cli": "^5.3.0",
        "babel-preset-react": "^6.24.1",
        "electron": "5.0.6",
        "electron-builder": "^21.0.11",
        "electron-webpack": "^2.7.4",
        "electron-webpack-ts": "^4.0.0",
        "svg2img": "^0.6.1",
        "to-ico": "^1.1.5",
        "tslint": "^6.1.1",
        "tslint-react": "^4.2.0",
        "typescript": "^3.8.3",
        "webpack": "~4.35.3",
        "yargs": "^15.3.1"
    },
    "build": {
        "appId": "cdjedai.file-renamer",
        "directories": {
            "buildResources": "resources",
            "output": "bin"
        }
    }
}