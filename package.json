{
    "name": "file-renamer",
    "version": "0.0.1",
    "license": "GPL-3.0-or-later",
    "description": "The best TV and Movie Renamer",
    "author": "Charles-Gael DJEDAI <charlesgael.djedai@outlook.com>",
    "scripts": {
        "dev": "electron-webpack dev",
        "compile": "electron-webpack",
        "dist": "yarn compile && electron-builder",
        "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
        "icons": "node ./scripts/svg2ico.js",
        "mksvg": "svgr -d src/components/SvgComponents/ svg/",
        "test": "run-s test:*",
        "test:types": "tsc --noEmit",
        "test:jest": "jest"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.8",
        "@material-ui/lab": "^4.0.0-alpha.47",
        "@types/classnames": "^2.2.10",
        "@types/dotenv": "^8.2.0",
        "@types/ent": "^2.2.1",
        "@types/flat": "^5.0.0",
        "@types/fs-extra": "^8.1.0",
        "@types/mustache": "^4.0.1",
        "@types/object-assign-deep": "^0.4.0",
        "@types/react": "^16.9.31",
        "@types/react-dom": "^16.9.6",
        "@types/react-router-dom": "^5.1.3",
        "axios": "^0.21.1",
        "classnames": "^2.2.6",
        "dotenv": "^8.2.0",
        "electron-debug": "^3.0.1",
        "electron-store": "^5.1.1",
        "electron-util": "^0.14.0",
        "flat": "^5.0.0",
        "fs-extra": "^9.0.0",
        "homedir": "^0.6.0",
        "object-assign-deep": "^0.4.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-dropzone": "^10.2.2",
        "react-icons": "^3.9.0",
        "react-router-dom": "^5.1.2",
        "react-scroll-sync": "^0.7.1",
        "rxjs-class-react-hook": "^1.0.8",
        "safe-eval": "^0.4.1",
        "source-map-support": "^0.5.12",
        "tmdb-v3": "^2.2.1",
        "windows-network-drive": "^3.0.1"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@babel/preset-typescript": "^7.9.0",
        "@fiahfy/icns-convert": "^0.0.8",
        "@svgr/cli": "^5.3.0",
        "@types/jest": "^25.2.1",
        "babel-preset-react": "^6.24.1",
        "electron": "^8.2.0",
        "electron-builder": "^21.0.11",
        "electron-webpack": "^2.7.4",
        "electron-webpack-ts": "^4.0.0",
        "jest": "^25.2.7",
        "npm-run-all": "^4.1.5",
        "svg2img": "^0.6.1",
        "to-ico": "^1.1.5",
        "ts-jest": "^25.3.1",
        "tslint": "^6.1.1",
        "tslint-react": "^4.2.0",
        "typescript": "^3.8.3",
        "webpack": "~4.35.3",
        "yargs": "^15.3.1"
    },
    "electronWebpack": {
        "whiteListedModules": [
            "react-router-dom",
            "@material-ui/core",
            "@material-ui/lab",
            "react-scroll-sync",
            "react-dropzone",
            "rxjs-class-react-hook"
        ]
    },
    "build": {
        "appId": "cdjedai.file-renamer",
        "directories": {
            "buildResources": "resources",
            "output": "bin"
        }
    }
}